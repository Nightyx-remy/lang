# [TodoList]
# - Enumeration

pub class List<T> {
    var data: &T;
    var size: usize;
    var length: usize;

    pub fn get_length(): usize {
        return self.length;
    }

    pub fn push(data: T) {
        while (self.length >= self.size) {
            self.size = self.size * 2;
        }

        @call("c") realloc(self.data, @call("c") sizeof(T) * self.size);
    }
}

fn main(): i32 {
    var my_list = @generic(i32) new List();
    var my_list = new List::<i32>();
}