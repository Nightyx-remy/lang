# [TodoList]
# self() / super()
# - Prototype
# - Enumeration

pub intf Aged {

    fn get_age(): u32;

}

pub proto Named {
    prot var name: str;
}

class Person: Aged + Named {
    var age: u32;

    fn new(name: str, age: u32): Person {
        self.name = name;
        self.age = age;
    }

    fn get_age(): u32 {
        return self.age;
    }

    fn to_string(): str {
        println("{} is {} years old", self.name, self.age);
    }
}

